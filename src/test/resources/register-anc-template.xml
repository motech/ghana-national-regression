        <xf:bind id="staffId" nodeset="/ANCRegistration/staffId" required="true()" type="xsd:int"
                 constraint=". &lt; 2147483647" message="Number too large. Keep under 2147483647"/>
        <xf:bind id="facilityId" nodeset="/ANCRegistration/facilityId" required="true()" type="xsd:int"
                 constraint=". &lt; 2147483647" message="Number too large. Keep under 2147483647"/>
        <xf:bind id="regDateToday" nodeset="/ANCRegistration/regDateToday" required="true()" type="xsd:string"/>
        <xf:bind constraint=". &lt;= 'today()'" id="date" message="Invalid date: date entered is in the future"
                 relevant="/ANCRegistration/regDateToday = 'IN_PAST' or /ANCRegistration/regDateToday = 'IN_PAST_IN_OTHER_FACILITY'"
                 visible="false()" action="show" nodeset="/ANCRegistration/date" required="true()" type="xsd:date"/>
        <xf:bind id="motechId" nodeset="/ANCRegistration/motechId" required="true()" type="xsd:int"
                 constraint=". &gt; 999999 and . &lt; 10000000" message="MoTeCH ID must be 7 digits long"/>
        <xf:bind id="ancRegNumber" nodeset="/ANCRegistration/ancRegNumber" type="xsd:string" required="true()"/>
        <xf:bind id="estDeliveryDate"  nodeset="/ANCRegistration/estDeliveryDate" required="true()" type="xsd:date"/>
        <xf:bind id="deliveryDateConfirmed" nodeset="/ANCRegistration/deliveryDateConfirmed" required="true()"
                 type="xsd:string"/>
        <xf:bind constraint=". &gt;= 60 and . &lt;= 200" id="height"
                 message="Value out of range: must be between 60 and 200" nodeset="/ANCRegistration/height"
                 required="true()" type="xsd:decimal"/>
        <xf:bind constraint=". &gt;= 1 and . &lt;= 30" id="gravida"
                 message="Value out of range: must be between 1 and 30" nodeset="/ANCRegistration/gravida"
                 required="true()" type="xsd:int"/>
        <xf:bind constraint=". &gt;= 0 and . &lt;= 30" id="parity"
                 message="Value out of range: must be between 0 and 30" nodeset="/ANCRegistration/parity"
                 required="true()" type="xsd:int"/>
        <xf:bind id="addHistory" nodeset="/ANCRegistration/addHistory" type="xsd:string" required="true()"/>
        <xf:bind id="addCareHistory" nodeset="/ANCRegistration/addCareHistory"
                 relevant="/ANCRegistration/addHistory = '1'" type="xsd:string" visible="false()" action="show"
                 required="true()"/>
        <xf:bind id="lastIPT" nodeset="/ANCRegistration/lastIPT" type="xsd:string" visible="false()"
                 relevant="/ANCRegistration/addCareHistory = 'IPT_SP' and /ANCRegistration/addHistory = '1'"
                 action="show" required="true()"/>
        <xf:bind id="lastIPTDate" nodeset="/ANCRegistration/lastIPTDate" type="xsd:date" visible="false()"
                 constraint=". &lt;= 'today()'" message="Invalid date: date specified is in the future"
                 relevant="/ANCRegistration/addCareHistory = 'IPT_SP' and /ANCRegistration/addHistory = '1'" action="show"
                 required="true()"/>
        <xf:bind id="lastTT" nodeset="/ANCRegistration/lastTT" type="xsd:string" visible="false()"
                 relevant="/ANCRegistration/addCareHistory = 'TT' and /ANCRegistration/addHistory = '1'"
                 action="show" required="true()"/>
        <xf:bind id="lastTTDate" nodeset="/ANCRegistration/lastTTDate" type="xsd:date" visible="false()" action="show"
                 constraint=". &lt;= 'today()'"
                 message="Invalid date: date specified is in the future"
                 relevant="/ANCRegistration/addCareHistory = 'TT' and /ANCRegistration/addHistory = '1'"
                 required="true()"/>
        <xf:bind id="enroll" nodeset="/ANCRegistration/enroll" required="true()" type="xsd:string"/>
        <xf:bind action="show" id="consent" nodeset="/ANCRegistration/consent" relevant="/ANCRegistration/enroll = 'Y'"
                 required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="serviceType" nodeset="/ANCRegistration/serviceType"
                 relevant="/ANCRegistration/consent = Y" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="phoneOwnership" nodeset="/ANCRegistration/phoneOwnership"
                 relevant="/ANCRegistration/consent = 'Y'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="mmRegPhone" nodeset="/ANCRegistration/mmRegPhone"
                 relevant="/ANCRegistration/phoneOwnership = 'PERSONAL' or /ANCRegistration/phoneOwnership = 'HOUSEHOLD'"
                 required="true()"
                 type="xsd:int" visible="false()" constraint=". &lt;= 999999999"
                 message="Number too large. It should be 10 digits long"/>
        <xf:bind action="show" id="format" nodeset="/ANCRegistration/format" relevant="/ANCRegistration/consent = 'Y'"
                 required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="language" nodeset="/ANCRegistration/language"
                 relevant="/ANCRegistration/consent = 'Y'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="dayOfWeek" nodeset="/ANCRegistration/dayOfWeek"
                 relevant="/ANCRegistration/format = 'PERS_VOICE' or /ANCRegistration/format = 'HSE_VOICE'"
                 required="true()"
                 type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="timeOfDay" nodeset="/ANCRegistration/timeOfDay"
                 relevant="/ANCRegistration/format = 'PERS_VOICE' or /ANCRegistration/format = 'HSE_VOICE'"
                 required="true()"
                 type="xsd:time" visible="false()"/>
        <xf:bind action="show" id="learnedFrom" nodeset="/ANCRegistration/learnedFrom"
                 relevant="/ANCRegistration/consent = 'Y'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="reasonToJoin" nodeset="/ANCRegistration/reasonToJoin"
                 relevant="/ANCRegistration/consent = 'Y'" required="true()" type="xsd:string"
                 visible="false()"/>
        <xf:bind action="show" id="messageStartWeek" nodeset="/ANCRegistration/messageStartWeek"
                 relevant="/ANCRegistration/enroll = 'Y' and /ANCRegistration/consent = 'Y'"
                 required="true()" type="xsd:string" visible="false()"/>
        <xf:bind id="formtype" nodeset="/ANCRegistration/formtype" required="true()" type="xsd:string"
                 visible="false()"/>
        <xf:bind id="formname" nodeset="/ANCRegistration/formname" required="true()" type="xsd:string"
                 visible="false()"/>
    </xf:model>
    <xf:group id="1">
        <xf:label>Page1</xf:label>
        <xf:input bind="staffId">
            <xf:label>Staff ID</xf:label>
        </xf:input>
        <xf:input bind="facilityId">
            <xf:label>Facility ID</xf:label>
        </xf:input>
        <xf:select1 bind="regDateToday">
            <xf:label>ANC Registration Today?</xf:label>
            <xf:item id="0">
                <xf:label>Today</xf:label>
                <xf:value>TODAY</xf:value>
            </xf:item>
            <xf:item id="1">
                <xf:label>In the past</xf:label>
                <xf:value>IN_PAST</xf:value>
            </xf:item>
            <xf:item id="2">
                <xf:label>In the past in other facility</xf:label>
                <xf:value>IN_PAST_IN_OTHER_FACILITY</xf:value>
            </xf:item>
        </xf:select1>
        <xf:input bind="date">
            <xf:label>Reg. date</xf:label>
        </xf:input>
        <xf:input bind="motechId">
            <xf:label>MoTeCH ID</xf:label>
        </xf:input>
        <xf:input bind="ancRegNumber">
            <xf:label>Serial Number</xf:label>
        </xf:input>
        <xf:input bind="estDeliveryDate">
            <xf:label>EDD</xf:label>
        </xf:input>
        <xf:select1 bind="deliveryDateConfirmed">
            <xf:label>Delivery date confirmed by CHW</xf:label>
            <xf:item id="Y">
                <xf:label>Yes</xf:label>
                <xf:value>Y</xf:value>
            </xf:item>
            <xf:item id="N">
                <xf:label>No</xf:label>
                <xf:value>N</xf:value>
            </xf:item>
        </xf:select1>
        <xf:input bind="height">
            <xf:label>Height (cm)</xf:label>
        </xf:input>
        <xf:input bind="gravida">
            <xf:label>Gravida</xf:label>
        </xf:input>
        <xf:input bind="parity">
            <xf:label>Parity</xf:label>
        </xf:input>
        <xf:select1 bind="addHistory">
            <xf:label>Add History</xf:label>
            <xf:item id="1">
