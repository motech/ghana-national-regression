        <xf:bind id="staffId" nodeset="/delivery/staffId" required="true()" type="xsd:int" constraint=". &lt; 2147483647" message="Number too large. Keep under 2147483647"/>
        <xf:bind id="facilityId" nodeset="/delivery/facilityId" required="true()" type="xsd:int" constraint=". &lt; 2147483647" message="Number too large. Keep under 2147483647"/>
        <xf:bind id="motechId" nodeset="/delivery/motechId" required="true()" type="xsd:int" constraint=". &gt; 999999 and . &lt; 10000000" message="MoTeCH ID must be 7 digits long"/>
        <xf:bind constraint=". &lt;= 'today()'" id="date" message="Invalid date: date specified is in the future" nodeset="/delivery/date" required="true()" type="xsd:dateTime"/>
        <xf:bind id="mode" nodeset="/delivery/mode" required="true()" type="xsd:string"/>
        <xf:bind id="outcome" nodeset="/delivery/outcome" required="true()" type="xsd:string"/>
        <xf:bind id="maleInvolved" nodeset="/delivery/maleInvolved" required="true()" type="xsd:string"/>
        <xf:bind id="deliveryLocation" nodeset="/delivery/deliveryLocation" required="true()" type="xsd:string"/>
        <xf:bind id="deliveredBy" nodeset="/delivery/deliveredBy" required="true()" type="xsd:string"/>
        <xf:bind id="complications" nodeset="/delivery/complications" type="xsd:string"/>
        <xf:bind action="show" id="vvf" nodeset="/delivery/vvf" relevant="/delivery/complications = '6'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind id="maternalDeath" nodeset="/delivery/maternalDeath" required="true()" type="xsd:string"/>
        <xf:bind id="comments" nodeset="/delivery/comments" type="xsd:string"/>
        <xf:bind action="show" id="child1Outcome" nodeset="/delivery/child1Outcome" relevant="/delivery/outcome = '1' or /delivery/outcome = '2' or /delivery/outcome = '3'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="child1RegistrationType" nodeset="/delivery/child1RegistrationType" relevant="/delivery/child1Outcome = 'A'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="child1MotechId" nodeset="/delivery/child1MotechId" relevant="/delivery/child1Outcome = 'A' and /delivery/child1RegistrationType = 'USE_PREPRINTED_ID'" required="true()" type="xsd:int" visible="false()" constraint=". &gt; 999999 and . &lt; 10000000" message="MoTeCH ID must be 7 digits long"/>
        <xf:bind action="show" id="child1Sex" nodeset="/delivery/child1Sex" relevant="/delivery/child1Outcome = 'A'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="child1FirstName" nodeset="/delivery/child1FirstName" relevant="/delivery/child1Outcome = 'A'" type="xsd:string" visible="false()" required="false()"/>
        <xf:bind action="show" id="child1Weight" nodeset="/delivery/child1Weight" relevant="/delivery/child1Outcome = 'A'" required="true()" type="xsd:decimal" visible="false()" constraint=". &gt;= 0.2 and . &lt;= 11" message="Value out of range: must be between 0.2 and 11"/>
        <xf:bind action="show" id="child2Outcome" nodeset="/delivery/child2Outcome" relevant="/delivery/outcome = '2' or /delivery/outcome = '3'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="child2RegistrationType" nodeset="/delivery/child2RegistrationType" relevant="/delivery/child2Outcome = 'A'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="child2MotechId" nodeset="/delivery/child2MotechId" relevant="/delivery/child2Outcome = 'A' and /delivery/child2RegistrationType = 'USE_PREPRINTED_ID'" required="true()" type="xsd:int" visible="false()" constraint=". &gt; 999999 and . &lt; 10000000" message="MoTeCH ID must be 7 digits long"/>
        <xf:bind action="show" id="child2Sex" nodeset="/delivery/child2Sex" relevant="/delivery/child2Outcome = 'A'" required="true()" type="xsd:string" visible="false()"/>
